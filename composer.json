{
    "name": "bear/query-repository",
    "description": "Resource query responsibility segregation",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "require": {
        "bear/resource": "~1.0@dev",
        "mobiledetect/mobiledetectlib": "~2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "BEAR\\QueryRepository\\": "src/"
        },
        "files": ["src-file/doctrine_annotations.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "BEAR\\QueryRepository\\": [
                "tests/",
                "tests/Fake/"
            ],
            "FakeVendor\\HelloWorld\\": "tests/Fake/fake-app/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
